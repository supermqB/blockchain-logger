    </parent>
    <groupId>com.lrhealth.bcos</groupId>
    <artifactId>blockchain-logger</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>blockchain-logger</name>
    <description>Demo project for Spring Boot</description>
    <properties>
        <java.version>8</java.version>
